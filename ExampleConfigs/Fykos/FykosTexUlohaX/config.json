{
    "variables": {
        "mainFolderName": "Serie",
        "taskDirName": "Uloha",
        "fileType": "tex",
        "author": "Daniel Sk√Ωpala",
        "taskBasicNames": ["1", "2", "3", "4", "5", "E", "S"],
        "taskSourcesNames": ["P"],

        "fileContentBasic": "{startFile}{author}{endFileBasic}",
        "fileContentSources": "{startFile}{author}{endFileSources}"
    },
    "fileVariables": {
        "startFile": "startFile.txt",
        "endFileBasic": "endFileBasic.txt",
        "endFileSources": "endFileSources.txt"
    },
    "scriptVariables": {
        "serie": {
            "filename": "findLowestFree",
            "func": "load",
            "args": ["{\\path}", "{mainFolderName}(\\d+)"]
        }
    },
    "dir": {
        "{mainFolderName}{serie}": {
            "taskBasic": "${taskBasicNames}",
            "taskSources": "${taskSourcesNames}"
        }
    },
    "templates": {
        "taskBasic": {
            "args": ["taskName"],
            "dir": {
                "{taskDirName}{taskName}": {
                    "{taskName}.{fileType}":  "{fileContentBasic}",
                    "images": {}
                }
            }
        },
        "taskSources": {
            "args": ["taskName"],
            "dir": {
                "{taskDirName}{taskName}": {
                    "{taskName}.{fileType}":  "{fileContentSources}",
                    "images": {},
                    "sources.bib": ""
                }
            }
        }
    }
}